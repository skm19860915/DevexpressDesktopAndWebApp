@model BlitzerCore.Models.DBVersion

@{
    ViewData["Title"] = "Version";
    Layout = "~/Views/Shared/_BlitzerLayout.cshtml";
}

<div class="offset-2 col-8">
    <h1>System Versions</h1>

    <div>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                Database
            </dt>
            <dd class="col-sm-10">
                @Model.Label
            </dd>
            <dt class="col-sm-2">
                Web Application
            </dt>
            <dd class="col-sm-10">
                @WebApp.Services.BlitzerServices.Label
            </dd>
            <dt class="col-sm-2">
                Google Traking
            </dt>
            <dd class="col-sm-10">
                @WebApp.ExtensionMethods.ViewExtensions.isRelease().ToString()
            </dd>
        </dl>
    </div>
</div>
<div>
